[x,fs] = audioread('q1.wav');
tmp = size(x)/fs;
s1 = x(:,1);
%figure(1);
%plot(s1);
%hold on
s1 = s1(1:60000);

[y,fs2] = audioread('r1.wav');
tmp2 = size(y)/fs2;
s2 = y(:,1);
%figure(2);
%plot(s2);
s2 = s2(1:60000);

N = 512;
Xa = fft(s1,N);
Xb = fft(s2,N);

X = [Xa, Xb];
T = [0 ,1];
net = newpr(X,T,20);

net =  train(net,X,T);